<div class="container">
  <h2>Products</h2>
  <div class="row">
<div class="container">
  <%= form_tag pages_path do %>
    <label for="file">File to upload</label> <%= file_field_tag "file" %>
    <div><%= submit_tag 'Submit' %></div>
  <% end %>
    <table class="table_style">
        <tr>
          <th>Title</th>
          <th>Type</th>
          <th>Rating</th>
          <th>Price</th>
          <th>Created</th>
          <th>Action</th>
        </tr>
        <% @products.each do |product| %>
        <tr>
        <td><%= product.title %></td>
        <td><%= product.product_type %></td>
        <td><%= product.rating %></td>
        <td><%= product.price %></td>
        <td><%= product.created_at %></td>
        <td><%= link_to "Edit", edit_page_path(product.id) %> <%= link_to "Delete", page_path(product.id), method: :delete %></td>
        <% end %>
    </table>
  </div>
</div>
</div>
